<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nhom28.quanlibanhang.pojo.BoPhan" table="bo_phan" catalog="quanlibanhang">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="tenBoPhan" type="string">
            <column name="ten_bo_phan" length="10" not-null="true" unique="true" />
        </property>
        <property name="idNguoiQuanLy" type="string">
            <column name="id_nguoi_quan_ly" length="20" not-null="true" unique="true" />
        </property>        
    </class>
</hibernate-mapping>
