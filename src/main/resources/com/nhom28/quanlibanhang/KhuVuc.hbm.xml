        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="tenKhuVuc" type="string">
            <column name="ten_khu_vuc" length="30" />
        </property>
        <property name="idNguoiQuanLy" type="java.lang.Integer">
            <column name="id_nguoi_quan_ly"/>
        </property>                    
    </class>
</hibernate-mapping>
