<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nhom28.quanlibanhang.pojo.NhanVien" table="nhan_vien" catalog="quanlibanhang">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="tenDayDu" type="string">
            <column name="ten_day_du" length="30" />
        </property>
        <property name="ngaySinh" type="date">
            <column name="ngay_sinh"/>
        </property>
        <property name="diaChi" type="string">
            <column name="dia_chi" length="30" />
        </property>
        <property name="dienThoai" type="string">
            <column name="dien_thoai" length="30" />
        </property>
        <property name="email" type="string">
            <column name="email" length="30" />
        </property>
        <property name="gioiTinh" type="java.lang.Integer">
            <column name="gioi_tinh" />
        </property>
        <property name="ngayBatDau" type="date">
            <column name="ngay_bat_dau" />
        </property>
        <property name="loaiNhanVien" type="java.lang.Integer">
            <column name="loai_nhan_vien" />
        </property>
        <property name="idBoPhan" type="java.lang.Integer">
            <column name="id_bo_phan" />
        </property>                 
    </class>
</hibernate-mapping>
